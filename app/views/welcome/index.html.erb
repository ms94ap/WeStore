<header>
  <div>
    <% if current_user && current_user.admin %>
    <%= link_to "categories", admin_categories_path %>
    <% end %>
    <h1>WeStore</h1>
    <h2>Welcome to the Wholesale E-Store</h2>

    <h4>Posts</h4>

    <ul>
      
      <li><% @posts.each do |f|%>
      <%= f.user.country%>
      <%= f.user.name%>
      <%= f.title %> <%= f.last_updated %>
          
        
          
      </li>
      <% end %>
    </ul>

  </div>
</header>
